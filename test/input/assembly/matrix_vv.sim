element Vertex
  a : int;
  b : int;
end

element Edge
end

extern V : set{Vertex};
extern E : set{Edge}(V,V);

func f(s : Edge, p : (Vertex*2)) -> Ae : tensor[V,V](int)
  Ae(p(0),p(0)) = 1;
  Ae(p(0),p(1)) = 1;
  Ae(p(1),p(0)) = 1;
  Ae(p(1),p(1)) = 1;
end

export func main()
  As = map f to E reduce +;
  V.b = As * V.a;
end
